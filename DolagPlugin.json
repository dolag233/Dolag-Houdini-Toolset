{
	"enable" : true,
	"load_package_once" : true,
	"env" :
	[
		{
			"HOUDINI_VEX_PATH" :
			{
				"value" : "$HOUDINI_USER_PREF_DIR/DolagPlugin/vex/custom",
				"method" : "prepend"
			}
		},
		{
			"DOLAG_HOUDINI_PATH" : "$HOUDINI_USER_PREF_DIR/DolagPlugin"
		},
		{
			"DOLAG_PATH" : "$HOUDINI_USER_PREF_DIR/DolagPlugin"
		},
		{
			"DOLAG_PYTHON_LIB" : "$HOUDINI_USER_PREF_DIR/DolagPlugin/python/python2.7libs"
		},
		{
			"DOLAG_PYTHON_SNIPPET_PATH" : "$HOUDINI_USER_PREF_DIR/DolagPlugin/python/python2.7libs/Dolag"
		},
		{
			"HOUDINI_OTLSCAN_PATH" : "$HOUDINI_USER_PREF_DIR/DolagPlugin/otls"
		},
		{
			"STICKER_PATH" :
			{
				"value" : "$HOUDINI_USER_PREF_DIR/DolagPlugin/stickers/",
				"method" : "prepend"
			},
			"$HOUDINI_PYTHON_PANEL_PATH" :
			{
				"value" : "$HOUDINI_USER_PREF_DIR/DolagPlugin/python_panels/",
				"method" : "prepend"
			}
		}
	],
	"path" : "$DOLAG_HOUDINI_PATH"
}